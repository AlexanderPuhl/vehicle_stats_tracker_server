{
	"extends": "airbnb",
	"rules": {
		"arrow-parens": [
			"error",
			"always"
		],
		"no-use-before-define": [
			"error",
			{
				"functions": false,
				"classes": false
			}
		],
		"no-underscore-dangle": [
			"error",
			{
				"allow": [
					"_id",
					"__v"
				]
			}
		],
		"no-param-reassign": 0,
		"linebreak-style": 0,
		"indent": [
			"error",
			"tab"
		],
		"no-tabs": 0,
		"quotes": [
			"single",
			{
				"allowTemplateLiterals": true
			}
		]
	}
}